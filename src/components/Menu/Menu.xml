<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">
    <fest:for iterate="json.buttons" index="i">
        <button type="button" class="{json.buttons[i].class}" data-url="{json.buttons[i].url}">
            <fest:value>json.buttons[i].text</fest:value>
        </button>
    </fest:for>
</fest:template>
