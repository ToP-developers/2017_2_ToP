<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">
    <table>
        <thead>
            <tr>
                <fest:for iterate="json.head" index="i">
                    <th><fest:value>json.head[i]</fest:value></th>
                </fest:for>
            </tr>
        </thead>
        <tbody class="table__body">
            <fest:for iterate="json.body" index="i">
                <tr>
                    <fest:for iterate="json.body[i]" index="j">
                        <td><fest:value>json.body[i][j]</fest:value></td>
                    </fest:for>
                </tr>
            </fest:for>
        </tbody>
    </table>
</fest:template>
